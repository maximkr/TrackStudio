<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.trackstudio.model.Umstatus" proxy="com.trackstudio.model.Umstatus" table="gr_umstatus">
        <cache usage="nonstrict-read-write"/>
        <id column="umstatus_id" name="id" type="java.lang.String" unsaved-value="null">
            <generator class="uuid.hex"/>
        </id>
        <property column="umstatus_type" name="type" type="java.lang.String"/>
        <many-to-one name="udf" column="umstatus_udf" class="com.trackstudio.model.Udf" cascade="none"/>
        <many-to-one name="mstatus" column="umstatus_mstatus" class="com.trackstudio.model.Mstatus" cascade="none"/>
    </class>
</hibernate-mapping>
