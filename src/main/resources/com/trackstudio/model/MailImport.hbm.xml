<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.trackstudio.model.MailImport" proxy="com.trackstudio.model.MailImport" table="gr_mailimport">
        <cache usage="nonstrict-read-write"/>

        <id column="mailimport_id" name="id" type="java.lang.String" unsaved-value="null">
            <generator class="uuid.hex"/>
        </id>
        <property column="mailimport_search_in" not-null="true" name="searchIn" type="java.lang.Integer"/>
        <property column="mailimport_keywords" not-null="false" name="keywords" type="string"/>
        <property column="mailimport_order" name="order" type="java.lang.Integer"/>
        <property column="mailimport_name" not-null="true" name="name" type="java.lang.String"/>
        <property column="mailimport_domain" not-null="false" name="domain" type="java.lang.String"/>
        <property column="mailimport_active" not-null="false" name="active" type="java.lang.Integer"/>
        <property column="mailimport_importUnknown" not-null="true" name="importUnknown" type="java.lang.Integer"/>

        <many-to-one name="category" column="mailimport_category" class="com.trackstudio.model.Category" cascade="none"/>
        <many-to-one name="task" column="mailimport_task" class="com.trackstudio.model.Task" cascade="none"/>
        <many-to-one name="owner" column="mailimport_owner" class="com.trackstudio.model.User" cascade="none"/>
        <many-to-one name="mstatus" column="mailimport_mstatus" class="com.trackstudio.model.Mstatus" cascade="none"/>

    </class>
</hibernate-mapping>
