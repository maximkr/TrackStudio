<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.trackstudio.model.CurrentFilter" proxy="com.trackstudio.model.CurrentFilter"
           table="gr_currentfilter">
        <cache usage="nonstrict-read-write"/>

        <id column="currentfilter_id" name="id" type="java.lang.String" unsaved-value="null">
            <generator class="uuid.hex"/>
        </id>
        <many-to-one name="task" column="currentfilter_task" class="com.trackstudio.model.Task" cascade="none"/>
        <many-to-one name="user" column="currentfilter_user" class="com.trackstudio.model.User" cascade="none"/>
        <many-to-one name="owner" column="currentfilter_owner" class="com.trackstudio.model.User" cascade="none"/>
        <many-to-one name="filter" column="currentfilter_fil" class="com.trackstudio.model.Filter" cascade="none"/>
    </class>
</hibernate-mapping>
