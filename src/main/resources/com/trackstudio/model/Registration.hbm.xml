<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.trackstudio.model.Registration" proxy="com.trackstudio.model.Registration" table="gr_registration">
        <cache usage="nonstrict-read-write"/>

        <id column="registration_id" name="id" type="java.lang.String" unsaved-value="null">
            <generator class="uuid.hex"/>
        </id>

        <property name="name" column="registration_name" not-null="true" type="java.lang.String"/>
        <property name="childAllowed" column="registration_child_allowed" type="java.lang.Integer"/>
        <property name="expireDays" column="registration_expire_days" type="java.lang.Integer"/>
        <property column="registration_priv" name="priv" type="java.lang.Integer"/>
        <many-to-one name="user" column="registration_user" class="com.trackstudio.model.User"/>
        <many-to-one name="prstatus" column="registration_prstatus" class="com.trackstudio.model.Prstatus"/>
        <many-to-one name="task" column="registration_task" class="com.trackstudio.model.Task"/>
        <many-to-one name="category" column="registration_category" class="com.trackstudio.model.Category"/>

    </class>
</hibernate-mapping>
