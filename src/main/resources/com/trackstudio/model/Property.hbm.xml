<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.trackstudio.model.Property" proxy="com.trackstudio.model.Property" table="gr_property">
        <cache usage="nonstrict-read-write"/>

        <id column="property_id" name="id" type="java.lang.String" unsaved-value="null">
            <generator class="uuid.hex"/>
        </id>
        <property column="property_name" not-null="true" name="name" type="java.lang.String"/>
        <property column="property_value" not-null="false" name="value" type="java.lang.String"/>

    </class>
</hibernate-mapping>
