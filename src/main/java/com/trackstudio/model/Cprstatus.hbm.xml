<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.trackstudio.model.Cprstatus" proxy="com.trackstudio.model.Cprstatus" table="gr_cprstatus">
        <cache usage="nonstrict-read-write"/>
        <id column="cprstatus_id" name="id" type="java.lang.String" unsaved-value="null">
            <generator class="uuid.hex"/>
        </id>
        <property column="cprstatus_type" name="type" type="java.lang.String"/>
        <many-to-one name="category" column="cprstatus_category" class="com.trackstudio.model.Category" cascade="none"/>
        <many-to-one name="prstatus" column="cprstatus_prstatus" class="com.trackstudio.model.Prstatus" cascade="none"/>
    </class>
</hibernate-mapping>
