<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.trackstudio.model.Uprstatus" proxy="com.trackstudio.model.Uprstatus" table="gr_uprstatus">
        <cache usage="nonstrict-read-write"/>
        <id column="uprstatus_id" name="id" type="java.lang.String" unsaved-value="null">
            <generator class="uuid.hex"/>
        </id>
        <property column="uprstatus_type" name="type" type="java.lang.String"/>
        <many-to-one name="udf" column="uprstatus_udf" class="com.trackstudio.model.Udf" cascade="none"/>
        <many-to-one name="prstatus" column="uprstatus_prstatus" class="com.trackstudio.model.Prstatus" cascade="none"/>
    </class>
</hibernate-mapping>
