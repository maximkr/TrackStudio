<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.trackstudio.model.Catrelation" proxy="com.trackstudio.model.Catrelation" table="gr_catrelation">
        <cache usage="nonstrict-read-write"/>
        <id column="catrelation_id" name="id" type="java.lang.String" unsaved-value="null">
            <generator class="uuid.hex"/>
        </id>
        <many-to-one name="category" column="catrelation_category" class="com.trackstudio.model.Category"
                     cascade="none"/>
        <many-to-one name="child" column="catrelation_child" class="com.trackstudio.model.Category" cascade="none"/>
    </class>
</hibernate-mapping>
